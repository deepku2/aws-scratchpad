{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowDataPlaneActions",
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:BatchGetItem",
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:BatchWriteItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem",
                "s3:Get*",
                "s3:List*",
                "s3:PutObject",
                "s3:DeleteObject",
                "sqs:SendMessage",
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sns:Publish",
                "events:PutEvents",
                "es:ESHttpGet",
                "es:ESHttpPut",
                "es:ESHttpPost",
                "es:ESHttpDelete",
                "rds-data:*",
                "rds:*",
                "opensearch:Get*",
                "opensearch:Search*",
                "opensearch:List*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "DenyControlPlaneAndIAMActions",
            "Effect": "Deny",
            "NotAction": [
                "dynamodb:GetItem",
                "dynamodb:BatchGetItem",
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:BatchWriteItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem",
                "s3:Get*",
                "s3:List*",
                "s3:PutObject",
                "s3:DeleteObject",
                "sqs:SendMessage",
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sns:Publish",
                "events:PutEvents",
                "es:ESHttpGet",
                "es:ESHttpPut",
                "es:ESHttpPost",
                "es:ESHttpDelete",
                "rds-data:*",
                "rds:*",
                "opensearch:Get*",
                "opensearch:Search*",
                "opensearch:List*"
            ],
            "Resource": "*"
        }
    ]
}
